{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "run-s build:css build:app",
    "build:app": "cross-env NODE_ENV=production remix build",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "build:sitemap": "esbuild ./app/lib/sitemap.ts --bundle --format=esm --define:process.env.NODE_ENV='\"development\"' --watch --platform=node",
    "build:storybook": "build-storybook",
    "dev": "run-p dev:*",
    "dev:remix": "cross-env NODE_ENV=development remix dev",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "watch:css": "postcss styles --base styles --dir app/styles -w",
    "test": "jest",
    "prepare": "husky install",
    "lint": "eslint 'app/**/*.{js,jsx,ts,tsx}' || exit 0",
    "lint:fix": "eslint 'app/**/*.{js,jsx,ts,tsx}' --fix",
    "fmt": "prettier --write 'app/**/*.{js,jsx,ts,tsx}' && eslint 'app/**/*.{js,jsx,ts,tsx}'",
    "lint-staged": "lint-staged",
    "storybook": "start-storybook -p 6006"
  },
  "dependencies": {
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
    "@remix-run/node": "^1.15.0",
