{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "ignorePresets": [":prHourlyLimit2"],
  "timezone": "Asia/Tokyo",
  "branchConcurrentLimit": 0,
  "ignorePaths": [
    ".github/workflows/*"
  ],
  "prHourlyLimit": 0,
  "prConcurrentLimit": 20,
  "labels": ["Renovate"],
  "commitMessagePrefix": "[Renovate] "
}
